<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="net.airbana.android.tabletmap"
      android:versionCode="1"
      android:versionName="1.0">
      <uses-permission android:name="android.permission.INTERNET"></uses-permission>
<uses-sdk android:minSdkVersion="11" />
<uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"></uses-permission>
<uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"></uses-permission>


    <application android:icon="@drawable/icon" android:label="Airsoft Map">
    <uses-library android:name="com.google.android.maps" />
        <activity android:name="net.airbana.android.map.Map"
                  android:label="Airsoft Map">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        <activity android:name="net.airbana.android.map.settingsLanding"
			android:label="@string/app_name"
			android:theme="@android:style/Theme.NoTitleBar">
		</activity>
		
		<activity android:name="net.airbana.android.map.LocationViewDetails"
			android:label="@string/app_name"
			android:theme="@android:style/Theme.NoTitleBar">
		</activity>

    </application>
</manifest>